{
	"name": "@kingstudios/config-generator",
	"version": "1.0.1",
	"description": "A CLI-based configuration generator for any TypeScript project. Edit configs in Google Sheets or CSV, compile to typed TypeScript modules.",
	"packageManager": "bun@1.3.6",
	"private": false,
	"type": "module",
	"bin": {
		"configgen": "dist/cli.js"
	},
	"files": [
		"dist/cli.js",
		"dist/cli.js.map",
		"README.md",
		"LICENSE"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/King-Studios-RBX/Configuration-Generator.git"
	},
	"author": "King Studios",
	"license": "MIT",
	"keywords": [
		"configuration",
		"config-generator",
		"google-sheets",
		"csv",
		"typescript",
		"cli"
	],
	"scripts": {
		"prepare": "bunx husky",
		"fetch": "bun run src/commands/fetch.ts",
		"build": "bun build src/cli.ts --outdir dist --target node --sourcemap=external && bun run src/commands/compile.ts",
		"compile": "bun run src/commands/compile.ts",
		"verify": "bun run src/commands/verify.ts",
		"sync": "bun run fetch && bun run compile",
		"lint": "bunx biome lint src tests",
		"test": "bun test"
	},
	"dependencies": {
		"csv-parse": "^6.1.0",
		"csv-stringify": "^6.6.0",
		"dotenv": "^17.2.3",
		"googleapis": "^170.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@commitlint/cli": "^20.3.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@ls-lint/ls-lint": "^2.3.1",
		"@types/bun": "latest",
		"cspell": "^9.6.0",
		"husky": "^9.1.7",
		"typescript": "^5.6.3"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"publishConfig": {
		"access": "public"
	}
}
